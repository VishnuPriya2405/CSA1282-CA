#include<stdio.h>
int main()
{
	int size,i,smallest,array[70];
	printf("enter size:");
	scanf("%d",&size);
	printf("enter elements:",size);
	for(i=0;i<size;i++)
	{
		scanf("%d",&array[i]);
	}
	smallest=array[0];
	for(i=1;i<size;i++)
	{
		if(smallest>array[i])
		smallest=array[i];
	}
	printf("smallest element is:%d",smallest);
	return 0;
}
